#include<stdio.h>
int main() 
{
    double n1, n2, n3;
    printf("Input the three numbers\n");
    scanf("%lf%lf%lf", &n1, &n2, &n3);
    if(n1>=n2&&n1>=n3)
    printf("The greatest number is : %lf", n1);
    if(n2>=n1&&n2>=n3)
    printf("The greatest number is : %lf", n2);
    if(n3>=n2&&n3>=n1)
    printf("The greatest number is : %lf", n3);
    return 0;
}
