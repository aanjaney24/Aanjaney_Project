#include<stdio.h>
int main() 
{
    int n1, n2;
    printf("Input the first number\n");
    scanf("%d", &n1);
    printf("Input the second number\n");
    scanf("%d", &n2);
    if(n1>n2)
    {
    printf("The first number that is %d is greater than the second number that is %d", n1, n2);
    }
    if(n2>n1)
    {
    printf("The second number that is %d is greater than the first number that is %d", n2, n1);
    } 
    return 0;
}
